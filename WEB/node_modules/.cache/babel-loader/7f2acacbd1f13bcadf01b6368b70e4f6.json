acer>\r\n              \r\n            </v-card-title>\r\n          </v-card>\r\n\r\n        </v-col>\r\n      </v-row>\r\n      <v-row>\r\n        \r\n        <v-col cols=\"4\" >\r\n        \r\n          <template >\r\n            <v-card style=\"min-height: 490px;\">\r\n              <v-card-title>\r\n                Quyền\r\n                <v-spacer></v-spacer>\r\n                <v-text-field\r\n                  v-model=\"search\"\r\n                  append-icon=\"mdi-magnify\"\r\n                  label=\"Tìm kiếm\"\r\n                  single-line\r\n                  hide-details\r\n                  clearable\r\n                ></v-text-field>\r\n                <v-spacer></v-spacer>\r\n                <v-btn\r\n                  color=\"primary\"\r\n                  @click=\"dialogAddRole=true\"\r\n                >\r\n                  Thêm\r\n                </v-btn>\r\n              </v-card-title>\r\n              <v-data-table\r\n                :headers=\"headers_role\"\r\n                :items=\"items_role\"\r\n                :search=\"search\"\r\n                :items-per-page=\"5\"\r\n              >\r\n              <template v-slot:body=\"{ items }\">\r\n                <tbody>\r\n                  <tr :class=\"key === selected_row ? 'custom-highlight-row' : ''\" \r\n                  @click=\"Role_Click_Row(item,key)\" \r\n                  v-for=\"(item, key) in items\" \r\n                  :key=\"item.stt\">\r\n                    <td>{{ key+1 }}</td>\r\n                    <td>{{ item.quyen_id }}</td>\r\n                    <td>{{ item.ten_quyen }}</td>\r\n                  </tr>\r\n                </tbody>\r\n              </template>\r\n              </v-data-table>\r\n            </v-card>\r\n          </template>\r\n              \r\n        </v-col>\r\n\r\n        <v-col cols=\"3.5\">\r\n           <template>\r\n            <v-card style=\"min-height: 490px;\">\r\n              <v-card-title>\r\n                Người dùng\r\n   